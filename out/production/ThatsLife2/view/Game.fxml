<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="rollButton" mnemonicParsing="false" onAction="#rollDice" text="Roll" />
            <Button mnemonicParsing="false" onAction="#onClickBackToMenu" text="Back to Menu" />
         </children>
      </HBox>
   </children>
</AnchorPane>
